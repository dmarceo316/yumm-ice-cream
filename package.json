{
  "name": "ice-cream",
  "version": "1.0.0",
  "description": "ice cream website",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "lite-server": "^2.5.4",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "imagemin-cli": "^5.1.0",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },
  "scripts": {
    "usemin": "usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin story.html -d dist --htmlmin -o dist/story.html && usemin flavors.html -d dist --htmlmin -o dist/flavors.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "imagemin": "imagemin img/* -o dist/img",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "lite": "lite-server",
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
